{{#articles}}
    <div class="post-bar">
        <div class="post_topbar">
            <div class="usy-dt">
                <div class="usy-name">
                    <span><img src="/images/clock.png" alt="">{{registrationDateTime}}</span>
                </div>
            </div>
        </div>
        <div class="job_descp">
            <h3>{{limitTitle}}</h3>
            <p>{{limitContent}}... <a href="/article/{{id}}" title="">view more</a>
            </p>
            <ul class="skill-tags">
                <li><a href="javascript:;" title="">{{articleTypeName}}</a></li>
            </ul>
        </div>
        <div class="job-status-bar">
            <ul class="like-com">
                <li>
                    <a href="javascript:;"><i class="la la-heart"></i>
                        Like {{likes}}</a>
                </li>
                <li>
                    <a><i class="la la-eye"></i>Views {{views}}</a>
                </li>
            </ul>
            <a><i class="la la la-bookmark"></i></a>
        </div>
    </div><!--post-bar end-->
{{/articles}}

<div class="process-comm">
    <div class="read-more">
        <a class="read-more-btn" href="javascript:;" onclick="readMore({{offset}})" data-offset="{{offset}}">Read More</a>
    </div>
</div><!--process-comm end-->

<div class="process-comm read-ready" style="display: none;">
    <div class="spinner">
        <div class="bounce1"></div>
        <div class="bounce2"></div>
        <div class="bounce3"></div>
    </div>
</div>
